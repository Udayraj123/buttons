<div class="effeckt effeckt-demo-buttons group">

  {{!-- demo metadata --}}
  <header>
    <h2> Example 2: Topcoat Buttons + Effeckt.css <a href="buttons.html">#</a> </h2>
    <span class="source">
      Source: <a href="http://lab.hakim.se/effeckt/"> Hakim El Hattab </a>
    </span>
  </header>
  
  {{!-- buttons --}}
  <div class="button-demo-wrap">
    {{> buttonPartial buttonFile.expand-right }}
  </div>
  
  <div class="button-demo-wrap">
    {{> buttonPartial buttonFile.expand-left }}
  </div>
  
  <div class="button-demo-wrap">
